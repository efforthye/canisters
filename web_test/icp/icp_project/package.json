{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.0 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy icp_project",
        "canister_deploy_mainnet": "dfx deploy --network ic icp_project",
        "canister_uninstall": "dfx canister uninstall-code icp_project",
        "canister_call_get_message": "dfx canister call icp_project getMessage",
        "canister_call_set_message": "dfx canister call icp_project setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "azle": "0.18.4"
    }
}

